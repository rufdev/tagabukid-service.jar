<schema>
	<element name="certification" tablename="certification">
		<field name="objid" primary="true"/>
		<field name="title"/>
		<field name="bldglocation"/>
		<field name="outdate"/>
		<field name="remarks"/>
		<field name="statuses"/>
		<field name="applications"/>
		<field name="txndate"/>
		<field name="certseq"/>
		<field name="preparedbyid"/>
		<field name="preparedbyname"/>
		<field name="state"/>
		<field name="recordlog_dateoflastupdate" />
		<field name="recordlog_lastupdatedbyuserid"/>
